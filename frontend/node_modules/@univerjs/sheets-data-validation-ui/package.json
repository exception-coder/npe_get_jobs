{
  "name": "@univerjs/sheets-data-validation-ui",
  "version": "0.11.0",
  "private": false,
  "description": "Data validation UI for Univer Sheets",
  "author": "DreamNum <developer@univer.ai>",
  "license": "Apache-2.0",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/univer"
  },
  "homepage": "https://univer.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/dream-num/univer"
  },
  "bugs": {
    "url": "https://github.com/dream-num/univer/issues"
  },
  "keywords": [
    "univer"
  ],
  "exports": {
    ".": {
      "import": "./lib/es/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/types/index.d.ts"
    },
    "./*": {
      "import": "./lib/es/*",
      "require": "./lib/cjs/*",
      "types": "./lib/types/index.d.ts"
    },
    "./locale/*": {
      "import": "./lib/es/locale/*.js",
      "require": "./lib/cjs/locale/*.js",
      "types": "./lib/types/locale/*.d.ts"
    },
    "./lib/*": "./lib/*"
  },
  "main": "./lib/es/index.js",
  "types": "./lib/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "peerDependencies": {
    "react": "^16.9.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^19.0.0-rc",
    "rxjs": ">=7.0.0"
  },
  "dependencies": {
    "@flatten-js/interval-tree": "^1.1.3",
    "@univerjs/icons": "^1.0.2",
    "@univerjs/core": "0.11.0",
    "@univerjs/data-validation": "0.11.0",
    "@univerjs/design": "0.11.0",
    "@univerjs/engine-formula": "0.11.0",
    "@univerjs/engine-render": "0.11.0",
    "@univerjs/sheets": "0.11.0",
    "@univerjs/sheets-numfmt": "0.11.0",
    "@univerjs/sheets-ui": "0.11.0",
    "@univerjs/sheets-formula-ui": "0.11.0",
    "@univerjs/sheets-data-validation": "0.11.0",
    "@univerjs/ui": "0.11.0"
  },
  "devDependencies": {
    "postcss": "^8.5.6",
    "react": "18.3.1",
    "rxjs": "^7.8.2",
    "tailwindcss": "3.4.18",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^3.2.4",
    "@univerjs-infra/shared": "0.11.0"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "build": "univer-cli build",
    "typecheck": "tsc --noEmit"
  },
  "module": "./lib/es/index.js"
}